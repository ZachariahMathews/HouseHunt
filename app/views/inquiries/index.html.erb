<p id="notice"><%= notice %></p>

<h1>Inquiries</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>House</th>
      <th>Subject</th>
      <th>Msg</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inquiries.each do |inquiry| %>
      <tr>
        <td><%= inquiry.user.name %></td>
        <td><%= inquiry.house.name %></td>
        <td><%= inquiry.subject %></td>
        <td><%= inquiry.msg %></td>
        <td><%= inquiry.created_at %></td>
        <td><%= link_to 'Show', inquiry %></td>
        <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
        <td><%= link_to 'Destroy', inquiry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inquiry', new_inquiry_path %> |
<%= link_to 'Back', welcome_index_path %>
